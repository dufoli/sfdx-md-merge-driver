{
  "name": "sfdx-md-merge-driver",
  "version": "0.1.2",
  "description": "git merge driver specific for Salesforce.com Metadata",
  "author": "jayree @jayree",
  "bin": {
    "sfdx-md-merge-driver": "./bin/run"
  },
  "bugs": "https://github.com/jayree/sfdx-md-merge-driver/issues",
  "man": "./sfdx-md-merge-driver.1",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.3",
    "mkdirp": "0.5.1",
    "pkg-dir": "4.2.0",
    "shelljs": "0.8.3",
    "tslib": "^1.10.0",
    "xml2js": "0.4.23"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@types/chai": "^4.2.7",
    "@types/mkdirp": "0.5.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.8",
    "@types/shelljs": "0.8.6",
    "@types/xml2js": "0.4.5",
    "@typescript-eslint/eslint-plugin": "2.16.0",
    "@typescript-eslint/parser": "2.16.0",
    "chai": "^4.2.0",
    "eslint": "6.8.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-prettier": "3.1.2",
    "globby": "^11.0.0",
    "husky": "^4.0.10",
    "lint-staged": "^9.5.0",
    "marked": "0.8.0",
    "marked-man": "^0.7.0",
    "mocha": "^7.0.0",
    "nyc": "15.0.0",
    "prettier": "1.19.1",
    "prettier-standard": "^16.1.0",
    "standard": "^14.3.1",
    "standard-version": "^7.0.1",
    "tap": "^14.10.6",
    "ts-node": "8.6.2",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/conf",
    "sfdx-md-merge-driver.1",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/jayree/sfdx-md-merge-driver",
  "keywords": [
    "sfdx",
    "salesforce",
    "metadata",
    "git",
    "merge",
    "driver",
    "conflict",
    "merge conflict",
    "resolve conflict",
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx-md-merge-driver",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "jayree/sfdx-md-merge-driver",
  "scripts": {
    "docs": "tail -n +2 README.md | marked-man --manual 'User Commands' --version \"$npm_package_name@$npm_package_version\" > sfdx-md-merge-driver.1",
    "prerelease": "yarn test",
    "postrelease": "npm publish && git push --follow-tags",
    "release": "standard-version",
    "uninstall": "node ./bin/run uninstall",
    "install": "node ./bin/run install",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "yarn prepack && nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
